{
    "name": "csp diabler",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Content Security Policy Disabler",
    "permissions": [
      "https://*.lightning.force.com/*",
      "https://*.visualforce.com/*",
      "webRequest",
      "webRequestBlocking"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    }
}
